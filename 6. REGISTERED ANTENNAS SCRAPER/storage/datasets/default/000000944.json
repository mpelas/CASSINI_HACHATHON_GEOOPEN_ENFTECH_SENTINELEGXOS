  "antenna_id": "146851",
  "address": "\"ΜΕΛΙΣΣΙΑ \" ΠΛΗΣΙΟΝ ΒΑΡΗΣ",
  "position_code": 1201059,
  "municipality": "Βάρης - Βούλας - Βουλιαγμένης",
  "code_name": "ΒΑΡΗ",
